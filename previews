<script>
/* Header behavior */
addEventListener('scroll',()=>document.querySelector('.header')
  .classList.toggle('scrolled',scrollY>40));

/* Mobile menu */
function toggleMenu(){
  let m=document.querySelector('nav');
  if(!m.classList.contains('nav-open')){ m.classList.add('nav-open'); m.style.display='flex'; }
  else{ m.classList.remove('nav-open'); m.removeAttribute('style'); }
}

/* Smooth in-page scroll */
document.addEventListener('click',e=>{
  const a=e.target.closest('a[href^="#"]'); if(!a) return;
  const t=document.querySelector(a.getAttribute('href')); if(!t) return;
  e.preventDefault(); t.scrollIntoView({behavior:'smooth',block:'start'});
});
</script>
</head>
