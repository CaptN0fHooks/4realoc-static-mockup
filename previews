<script>
/* Header shrink */
addEventListener('scroll',()=>document.querySelector('.header')
  .classList.toggle('scrolled',scrollY>40));

/* Mobile menu */
function toggleMenu(){
  const n=document.querySelector('nav');
  if(!n.classList.contains('nav-open')){ n.classList.add('nav-open'); n.style.display='flex'; }
  else{ n.classList.remove('nav-open'); n.removeAttribute('style'); }
}

/* Smooth anchor scroll */
document.addEventListener('click',e=>{
  const a=e.target.closest('a[href^="#"]'); if(!a) return;
  const t=document.querySelector(a.getAttribute('href')); if(!t) return;
  e.preventDefault(); t.scrollIntoView({behavior:'smooth',block:'start'});
});

/* Lightweight 3D tilt for cards */
(function(){
  const cards=[...document.querySelectorAll('.card')];
  const damp=30; // lower = stronger tilt
  cards.forEach(c=>{
    c.addEventListener('mousemove',e=>{
      const r=c.getBoundingClientRect();
      const x=(e.clientX-r.left)/r.width, y=(e.clientY-r.top)/r.height;
      const rx=(.5-y)*damp, ry=(x-.5)*damp;
      c.style.transform=`rotateX(${rx}deg) rotateY(${ry}deg) translateZ(0)`;
    });
    c.addEventListener('mouseleave',()=>{ c.style.transform='rotateX(0deg) rotateY(0deg)';});
  });
})();
</script>
